{
    "name": "kenkataiwa/oauth",
    "type": "package",
    "description": "Open Authentiation Abstration",
    "keywords": ["oauth", "php-oauth", "social integration"],
    "homepage": "http://www.github.com/kenkataiwa/oauth",
    "license": "MIT",
    "authors": [
        {"name": "Kenneth Kataiwa", "email": "kenkataiwa@gmail.com"}
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">= 5.4",
        "facebook/php-sdk": "3.2.2"
    },
    "require-dev": {
        "opauth/opauth": "dev-master",
        "opauth/facebook": "dev-master",
        "opauth/twitter": "dev-master",
        "opauth/google": "dev-master",
        "opauth/linkedin": "dev-master",
        "hybridauth/hybridauth": "dev-master"
    },
    "autoload": {
        "psr-0": {"OAuth": "lib/"}
    },
    "suggest": {
        "google/google-api-php-client": "Add a custom repository to http://google-api-php-client.googlecode.com/files/google-api-php-client-0.6.0.tar.gz "
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
